echo start OpenSplice

$VG_OSPL_START ospl start

sleep 10

echo start MessageBoard
$VG_MESSAGEBOARD java -classpath $OSPL_HOME/jar/dcpssaj.jar:bld chatroom.MessageBoard &

sleep 2

echo start Chatter
$VG_CHATTER java -classpath $OSPL_HOME/jar/dcpssaj.jar:bld chatroom.Chatter

sleep 4

echo start Chatter with terminate message
$VG_CHATTER_T java -classpath $OSPL_HOME/jar/dcpssaj.jar:bld chatroom.Chatter -1

sleep 4


$VG_OSPL_STOP ospl stop
