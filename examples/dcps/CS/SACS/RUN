$VG_OSPL_START ospl start
$VG_START_SLEEP

BLOKSIZE=100
BLOKCOUNT=100

$VG_PONG exec/$SPLICE_TARGET/pong PongRead PongWrite &

sleep 2

$VG_PING_M exec/$SPLICE_TARGET/ping $BLOKCOUNT $BLOKSIZE m PongRead PongWrite

sleep 2

$VG_PING_Q exec/$SPLICE_TARGET/ping $BLOKCOUNT $BLOKSIZE q PongRead PongWrite

sleep 2

$VG_PING_S exec/$SPLICE_TARGET/ping $BLOKCOUNT $BLOKSIZE s PongRead PongWrite

sleep 2

$VG_PING_B exec/$SPLICE_TARGET/ping $BLOKCOUNT $BLOKSIZE b PongRead PongWrite

sleep 2

$VG_PING_F exec/$SPLICE_TARGET/ping $BLOKCOUNT $BLOKSIZE f PongRead PongWrite

sleep 2

$VG_PING_T exec/$SPLICE_TARGET/ping 1  10 t PongRead PongWrite 

sleep 4

$VG_OSPL_STOP ospl stop
